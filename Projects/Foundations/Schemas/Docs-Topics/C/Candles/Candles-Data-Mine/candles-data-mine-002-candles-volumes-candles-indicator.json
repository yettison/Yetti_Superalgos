                {
                    "language": "DE",
                    "text": "Typische Kerzen.",
                    "updated": 1641654330857
                },
                {
                    "language": "TR",
                    "text": "Tipik mum çubukları.",
                    "updated": 1644500491024
                },
                {
                    "language": "RU",
                    "text": "Характерные свечи.",
                    "updated": 1645606630906
                }
            ]
